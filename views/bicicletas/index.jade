extends ../layout 

block content
    - var title = locals.title
    - var bicis = locals.bicis
    h1=title
    table.table
        thead 
            tr 
                th(scope='col') #
                th(scope='col') Color
                th(scope='col') Modelo
                th(scope='col') Ubicacion
        tbody 
            each bici in locals.bicis
                tr 
                    td(scope='row')
                        =bici.id
                    td=bici.color
                    td=bici.modelo
                    td=bici.ubicacion